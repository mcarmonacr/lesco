<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.lesco.diccionario.model">
    <class name="PreferredWord" table="PreferredWord">
    
        <id name="preferredWordId" type="java.lang.Integer" column="PreferredWord_ID">
            <generator class="native"/>
        </id>

		<property name="wordId" column="Word_ID" />
		<property name="userProfileId" column="UserProfile_ID" />
        
    </class> 
</hibernate-mapping>